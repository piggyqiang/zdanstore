{% extends "layouts/base_with_sidebar.twig" %}
{% set menu_name="合作伙伴"%}
{% set sidebar_name = "管理" %}
{% block content %}
<div class="container-fluid">
    <div class="row-fluid">
    {{ void(this.widget('bootstrap.widgets.TbGridView',{
            'id':'app-grid', 
            'dataProvider':model.search(), 
            'filter':model, 
            'columns':{
                0:'id',
                1:'cpname',
                2:'email',
                3:'created',
                4:'modified',
                5:
                {
                     'class':'bootstrap.widgets.TbButtonColumn',
                     'template':' {view}{update} {delete}',
                }
            },})) 
         }} 
  </div>
</div>
{% endblock %}