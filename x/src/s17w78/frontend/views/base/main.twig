<link rel="Bookmark" href="favicon.ico">

<script type="text/javascript" src="{{ App.assets.Url}}/js/zdan.js"></script>
<script type="text/javascript" src="{{ App.assets.Url}}/js/image.js"></script>
</head>
<body>
<div class="header clearfix">
	<div class="fl">
        <div class="search">
            <input type="text" name="" value="SEARCH..." class="search_ipt">
            <input type="submit" name="" value="&nbsp;" class="search_btn">
        </div>
        <h1 class="logo"><a href="/"></a></h1> 
    </div>
    <div class="fr">
        <div class="had">FREE SHIPPING ON ALL ORDRES</div>
        <ul class="hnav">
            <li class="i1"><a href="">Live chat!</a></li>
            {% if u.username is empty %}
            <li class="i2"><a href="/user/login">Sign in</a> | <a href="/user/register">Register</a></li>
            {% else %}
            <li class="i2">{{ u.username }} | <a href="/user/logout">Quit</a></li>
            {% endif %}
            <li class="i3"><a href="/shopping/cart">View cart</a></li>
        </ul>
        <!-- cart dialog -->
        <div class="cart_dialog">
            <a class="close">×</a>
            <div class="title">Add to Cart</div>
            <div class="content">
                <div id="cart_back"></div>
                <p class="tip">Free Shipping By Internatinoal Air Mail !</p>
                <div class="btn clearfix">
                    <a href="/shopping/cart" class="fr">View Cart</a>
                    <a href="#" class="fr">Continue Shopping</a>
                    <a href="/shopping/cart" class="fr"><img src="images/cart_ico.png" align="absmiddle" /> Checkout</a>
                </div>
            </div>
        </div>
        <!-- cart dialog -->
    </div>
</div>
{{ void(this.widget('MenuWidget')) }}

{% block content %}
{% endblock %}

<div class="footer clearfix">
    <div class="copyright">
        <div class="search_order">
            <span class="txt">Track Your Orders&gt;&gt;</span>
            <span class="order">
                <input type="text" name="" value="Tracking Numbers" class="email_ipt">
                <input type="submit" name="" value="GO!" class="email_btn">
            </span>
        </div>
        <div class="share">
            <a href="#" title=""><img src="images/share_ico1.png" /></a>
            <a href="#" title=""><img src="images/share_ico2.png" /></a>
            <a href="#" title=""><img src="images/share_ico3.png" /></a>
            <a href="#" title=""><img src="images/share_ico4.png" /></a>
        </div>
        <p>&copy 2005-2014 Zdanstore.com Copyright. All Rights Reserved.</p>
    </div>
    <div class="link">
        <dl>
            <dt>SHOP BY</dt>
            <dd><a href="#" title="">Brand</a></dd>
            <dd><a href="#" title="">Category</a></dd>
            <dd><a href="#" title="">Sale</a></dd>
            <dd><a href="#" title="">New Arrival</a></dd>
            <dd><a href="#" title="">Men's Clothing</a></dd>
            <dd><a href="#" title="">Women's clothing</a></dd>
        </dl>
        <dl>
            <dt>SHIPPING</dt>
            <dd><a href="#" title="">Returns</a></dd>
            <dd><a href="#" title="">Shipping Time</a></dd>
            <dd><a href="#" title="">Order Tracking</a></dd>
            <dd><a href="#" title="">Shipping Options</a></dd>
            <dd><a href="#" title="">Insurance</a></dd>
        </dl>
        <dl>
            <dt>HELP</dt>
            <dd><a href="#" title="">About Us</a></dd>
            <dd><a href="#" title="">F.Q.A.</a></dd>
            <dd><a href="#" title="">Contact Us</a></dd>
            <dd><a href="#" title="">Privacy Policy</a></dd>
        </dl>
    </div>
</div>

<!-- dialog code -->
<div class="popmark"></div>
<div id="popup" class="clearfix">
    <a href="javascript:void(0);" class="popclose">×</a>
    <div class="popcontent">
        
    </div>
</div>
<script type="text/javascript">
    $("ul li div.pic").mouseover(function(){
        $(this).find(".quick").show();
    });
    $("ul li div.pic").mouseout(function(){
        $(this).find(".quick").hide();
    });

    $(".quick").click(function(){
        popup();
        var id = $(this).parent().find("input").val();
        var cartData = {"id":id};
        $.ajax({
            type: "POST",
            url:"/product/quick",
            data:cartData,
            dataType:'json',
            success:function(data){     
                $(".popcontent").html(data); 
            }
        });
    });

    $(".popclose").click(function(){
        closePopup();
    });
    $(".cart_dialog .close").click(function(){
        $(".cart_dialog").hide();  
    });
</script>

</body>
</html>